{
  "name": "My Ext",
  "description": "Learn build an ext",
  "version": "0.1.0",
  "manifest_version": 2,
  "icons": {
    "16": "./imgs/icons8-pokeball-16.png",
    "32": "./imgs/icons8-pokeball-32.png",
    "48": "./imgs/icons8-pokeball-48.png",
    "128": "./imgs/icons8-pokeball-96.png"
  },
  "background": {
    "scripts": [
      "./background.js"
    ]
  },
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://www.google.com/*"
  ]
}